{
    "type": "postgres",
    "url": "YOUR_SUPABASE_POSTGRESQL_URL",
    "synchronize": true,
    "logging": true,
    "entities": [
        "dist/entities/**/*.js"
    ],
    "migrations": [
        "dist/migrations/**/*.js"
    ],
    "subscribers": [
        "dist/subscribers/**/*.js"
    ],
    "cli": {
        "entitiesDir": "src/entities",
        "migrationsDir": "src/migrations",
        "subscribersDir": "src/subscribers"
    }
}